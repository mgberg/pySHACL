# The following report is not the direct output from pyshacl- it has been formatted to improve readability for the sake of example.
[
  a sh:ValidationReport ;
  sh:conforms false ;

  # One error for ex:SomeUSCitizen with one nested error
  sh:result [
      a sh:ValidationResult ;
      sh:focusNode ex:SomeUSCitizen ;
      sh:resultMessage "Value ex:SomeUSCitizen does not conform to shape(s) at path ex:isCitizenOf/ex:hasCitizenRequirements. See details for more information." ;
      sh:resultPath ( ex:isCitizenOf ex:hasCitizenRequirements ) ;
      sh:resultSeverity sh:Violation ;
      sh:sourceConstraintComponent sh:NodesPathConstraintComponent ;
      sh:sourceShape ex:Person ;
      sh:value ex:SomeUSCitizen ;
      sh:detail [
          a sh:ValidationResult ;
          sh:focusNode ex:SomeUSCitizen ;
          sh:resultMessage "Less than 1 values on ex:SomeUSCitizen->ex:ssn" ;
          sh:resultPath ex:ssn ;
          sh:resultSeverity sh:Violation ;
          sh:sourceConstraintComponent sh:MinCountConstraintComponent ;
          sh:sourceShape [
              a sh:PropertyShape ;
              sh:path ex:ssn ;
              sh:datatype xsd:integer ;
              sh:minCount 1 ;
              sh:name "Social Security Number" ;
            ] ;
        ] ;
    ] ;

